---
import { ProjectCard } from "./ProjectCard";

const {
  title,
  description,
  technologies,
  demoURL,
  repoURL,
  image,
  imagesDetail,
} = Astro.props;
---

<ProjectCard
  client:load
  title={title}
  description={description}
  technologies={technologies}
  demoURL={demoURL}
  repoURL={repoURL}
  image={image}
  imagesDetail={imagesDetail}
/>
